{
  "name": "demo",
  "version": "1.0.0",
  "description": "pwa react demo",
  "main": "index.js",
  "homepage": "/novel/home",
  "scripts": {
    "postinstall": "patch-package",
    "start": "react-app-rewired start",
    "dev": "react-app-rewired start",
    "build": "CI=false PUBLIC_URL=/novel/ react-app-rewired build",
    "build-windows": "set PUBLIC_URL=/novel/&&react-app-rewired build && rm -rf ../node-mongo-novel/public/novel/* && cp -rf build/* ../node-mongo-novel/public/novel/",
    "build-dev": "PUBLC_URL=/novel/ react-app-rewired build",
    "test": "react-app-rewired test",
    "lint": "eslint ./src ./mock",
    "fix": "eslint ./src ./mock --fix",
    "mock": "cd mock && nodemon index.js"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      [
        "import",
        {
          "libraryName": "antd-mobile",
          "libraryDirectory": "lib"
        },
        "antd-mobile"
      ]
    ]
  },
  "dependencies": {
    "@csstools/normalize.css": "^10.1.0",
    "alloyfinger": "^0.1.16",
    "antd-mobile": "^2.3.1",
    "axios": "^0.19.0",
    "better-scroll": "^1.15.2",
    "bowser": "^2.7.0",
    "customize-cra": "^0.9.1",
    "date-fns": "^1.30.1",
    "device-detector-js": "^3.0.3",
    "events": "^3.3.0",
    "keyboardjs": "^2.5.1",
    "localforage": "^1.7.3",
    "mem": "^6.1.0",
    "mobx": "^5.10.1",
    "mobx-react-lite": "^1.4.0",
    "mobx-state-tree": "^3.14.0",
    "path-to-regexp": "^3.1.0",
    "qs": "^6.7.0",
    "react": "^16.8.6",
    "react-app-rewired": "^2.1.3",
    "react-clipboard.js": "^2.0.13",
    "react-dom": "^16.8.6",
    "react-flv-player": "^1.0.9",
    "react-helmet": "^5.2.1",
    "react-hls-player": "^3.0.7",
    "react-icons": "^3.7.0",
    "react-lazy-load-image-component": "^1.6.0",
    "react-photo-view": "^1.2.3",
    "react-router-dom": "^5.0.1",
    "react-scripts": "^3.0.1",
    "react-transition-group": "^4.4.1",
    "react-use": "^9.7.1",
    "react-use-gesture": "^9.0.4",
    "rebound": "^0.1.0",
    "screenfull": "^4.2.1",
    "socket.io-client": "^2.3.1",
    "styled-components": "^5.1.1",
    "workbox-build": "^6.5.2",
    "workbox-cacheable-response": "^6.5.2",
    "workbox-core": "^6.5.2",
    "workbox-expiration": "^6.5.2",
    "workbox-precaching": "^6.5.2",
    "workbox-recipes": "^6.5.2",
    "workbox-routing": "^6.5.2",
    "workbox-strategies": "^6.5.2",
    "workbox-streams": "^6.5.2",
    "workbox-sw": "^6.5.2",
    "workbox-webpack-plugin": "^6.5.2",
    "workbox-window": "^6.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "babel-plugin-import": "^1.12.1",
    "eslint": "^6.8.0",
    "express": "^4.17.1",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "mockjs": "^1.0.1-beta3",
    "nodemon": "^1.19.3",
    "patch-package": "^7.0.0",
    "prettier": "^3.2.5",
    "webpack-bundle-analyzer": "^3.6.0",
    "workbox-cli": "^6.5.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js}": [
      "prettier --write",
      "git add"
    ]
  },
  "keywords": [
    "pwa",
    "react",
    "demo"
  ],
  "author": "1024",
  "license": "ISC",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
