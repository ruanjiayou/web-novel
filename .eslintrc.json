{
  "env": {
    "es6": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 9,
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
  "globals": {},
  "rules": {
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "off",
      "always"
    ]
  }
}