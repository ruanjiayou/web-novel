{
  "env": {
    "es6": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 9,
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
  "globals": {},
  "rules": {
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "never"
    ],
    "no-extra-semi": "error",
    "no-unused-vars": "off",
    "no-restricted-globals": "off"
  }
}